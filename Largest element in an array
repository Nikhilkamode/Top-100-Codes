#include<bits.h>
using namespace std;
int largest element(int n,int arr[])
{
int max_elemenrt=INT _MIN;
for(int i=0;i<n;i++){
max_element=max(arr[i],max_element);
}
return max_element;
}
int main()
{
int array[]={10,20,30,40,50}
int n=sizeof(arr)/size(arr[0]);
cout<<"Largest element is "<<largest_element(n,arr);
}
