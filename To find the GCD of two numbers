Let's solve using cpp
Method 1:
#include<iostream.h>
using namespace std;
int main()
{
int num1=18,num2=45,gcd=1;
for(int i=1;i<=n;i++)
   {
if(num1%i==0||num2%i==0)
gcd=i;
   }
Cout<<"The Gcd is:"<<gcd;
return 0;
}

Method 2:
#include<iostream>
using namespace std;

// Using Euclidean Algorithm (Repeated Subtraction)
int main()
{
    int n1 = 104, n2 = 24, gcd = 1;
    while (n1 != n2)
    {
        if (n1 > n2)
            n1 -= n2;
        else
            n2 -= n1;
    }   
    cout<<"The GCD : "<<n1;
    
    return 0;
}
