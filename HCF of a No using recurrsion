#include<bits.h>
using namespace std;
int get hcf(intnum1,intnum2)
{
if(num1==0)
return num2;
if(num2==0)
return num1;
if(num1==num2)
return num1;
if(num1>num2)
return get hcf(num1-num2,num2);
return get hcf(num1,num2-num1);
}
int main()
{
int num1=96,intn num2=56,hcf=1;
HCF=get hcf(num1,num2);
cout<<"HCF of"<<num1<<and<<num2<<"is"<<hcf;
return0;
}


